<componentlayout>
<processes>
<process name="TextGenProcess" id="GENPROCESS">
  <description></description>
  <thread name="TextGenThread" id="TEXTGENTHREAD">
    <component name="TextGen1" id="TEXTGEN1" xml="textgen/TextGenComp.xml">
      <provides>
        <interface id="TextGenXml" pub="inproc:///tmp/hurtmexml" req="inproc:///tmp/hurtmexmlsync" hwm="5000"/>
        <interface id="TextGenPlain" pub="inproc:///tmp/hurtmeplain" req="inproc:///tmp/hurtmeplansync" hwm="5000"/>
      </provides>
    </component>
    <component name="TextGen2" id="TEXTGEN2" xml="textgen/TextGenComp.xml">
      <provides>
        <interface id="TextGenXml" pub="inproc:///tmp/hurtmexml2" req="inproc:///tmp/hurtmexml2sync"  hwm="2500"/>
        <interface id="TextGenPlain" pub="inproc:///tmp/hurtmeplain2" req="inproc:///tmp/hurtmeplain2sync" hwm="2500"/>
      </provides>
    </component>
    
