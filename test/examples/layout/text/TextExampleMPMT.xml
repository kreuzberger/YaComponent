        <if id="" maj="" min="" rev=""/>
      </uses>
      -->
  </thread>
</process>
<process name="TextOutProcess" id="OUTPROCESS">
  <description></description>
  <component name="TextOut" id="TEXTOUT" xml="textgen/TextOutComp.xml">
    <uses>
      <interface id="Xml" sub="TextGen1.TextGenXml"/>
    </uses>
  </component>
</process>
</processes>
</componentlayout>
