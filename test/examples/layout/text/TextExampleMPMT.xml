<componentlayout>
<processes>
<process name="TextGenProcess" id="GENPROCESS">
  <description></description>
  <thread name="TextGenThread" id="TEXTGENTHREAD">
    <component name="TextGen1" id="TEXTGEN1" xml="textgen/TextGenComp.xml">
      <provides>
        <interface id="TextGenXml" address="inproc:///tmp/hurtmexml"/>
        <interface id="TextGenPlain" address="inproc:///tmp/hurtmeplain"/>
      </provides>
    </component>
    <component name="TextGen2" id="TEXTGEN2" xml="textgen/TextGenComp.xml">
      <provides>
        <interface id="TextGenXml" address="inproc:///tmp/hurtmexml2"/>
        <interface id="TextGenPlain" address="inproc:///tmp/hurtmeplain2"/>
      </provides>
    </component>
    
